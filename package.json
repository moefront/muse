{
  "name": "ymplayer",
  "version": "4.1.0",
  "description": "Just a simple and diligent HTML5 audio player made with love.",
  "main": "dist/assets/ymplayer.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "demo": "node server.js --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "setup": "npm install -g cnpm && cnpm install",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": "https://github.com/kirainmoe/ymplayer",
  "keywords": [
    "ymplayer",
    "HTML5",
    "audio",
    "lyric"
  ],
  "author": "kirainmoe",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^7.0.0",
    "file-loader": "^0.11.1",
    "karma": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^2.0.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.0",
    "node-sass": "^4.0.0",
    "open": "0.0.5",
    "prepack-webpack-plugin": "^1.1.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.6",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
